{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/express/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/api/flask/(.*)",
        "dest": "main.py"
      }
    ]
  }